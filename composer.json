{
    "name": "katsana/remote-control",
    "description": "Grant remote access to user account without sharing credentials",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "KATSANA Developer",
            "email": "developer@katsana.com"
        },
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@katsana.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "RemoteControl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RemoteControl\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "illuminate/database": "^5.8 || ^6.0",
        "illuminate/mail": "^5.8 || ^6.0",
        "illuminate/support": "^5.8 || ^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.3",
        "orchestra/testbench": "^3.8.5 || ^4.0",
        "phpunit/phpunit": "^8.3"
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit -c ."
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        },
        "laravel": {
            "providers": [
                "RemoteControl\\RemoteServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
